                    <li><a href="ModuleList"><span class="fa fa-caret-right"></span> Module List</a></li>
                    <li><a href="NewUSer1"><span class="fa fa-caret-right"></span> New User</a></li>
                    <li><a href="UserList"><span class="fa fa-caret-right"></span> User List</a></li>
                    <li><a href="NewGroup1"><span class="fa fa-caret-right"></span> New Group</a></li>
                    <li><a href="GroupList"><span class="fa fa-caret-right"></span> Group List</a></li>
                </ul>
            </li>

            <li><a href="#" data-target=".reception-menu" class="nav-header" data-toggle="collapse">Reception<i class="fa fa-collapse"></i></a></li>
            <li>
                <ul class="reception-menu nav nav-list collapse">
                    <li><a href="#"><span class="fa fa-caret-right"></span> New Patient</a></li>
                    <li><a href="#"><span class="fa fa-caret-right"></span> Patient List</a></li>
                    <li><a href="#"><span class="fa fa-caret-right"></span> Reservation</a></li>
